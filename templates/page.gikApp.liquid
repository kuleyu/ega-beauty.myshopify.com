{% layout none %}
<style>
  .rte {
    color: #788188;
    margin-bottom: 35px;
  }

  img {
    max-width: 100%;
    height: auto;
    border: 0;
  }
  .section-header {
    margin-bottom: 35px;
  }
  .text-center {
    text-align: center !important;
  }
  h1, .h1, h2, .h2, h3, .h3, h4, .h4, h5, .h5, h6, .h6 {
    margin: 0 0 17.5px;
    font-family: "Work Sans","HelveticaNeue","Helvetica Neue",sans-serif;
    font-weight: 600;
    line-height: 1.2;
    overflow-wrap: break-word;
    word-wrap: break-word;
  }
  .article__author {
    margin-right: 10px;
  }
  p {
    color: #788188;
    font-size: 0.9375em;
    line-height: 1.5;
    font-family: "Work Sans","HelveticaNeue","Helvetica Neue",sans-serif;
  }
  blockquote {
    font-size: 1.125em;
    font-style: normal;
    text-align: center;
    padding: 0 30px;
    margin: 0;
    border-color: #e8e9eb;
    border-width: 1px 0;
    border-style: solid;
    padding: 30px 0;
    margin-bottom: 27.5px;
  }
</style>
<div id = "gikApp_artilce_content">
<article class="page-width" itemscope itemtype="http://schema.org/Article">
  <div class="grid">
    <div class="grid__item medium-up--five-sixths medium-up--push-one-twelfth">
      {{page.content}}
    </div>
  </div>
</article>

</div>